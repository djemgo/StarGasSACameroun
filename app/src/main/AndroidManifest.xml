            android:label="@string/title_activity_customer_orders"></activity>
        <activity
            android:name=".activity.CustomerProfileActivity"
            android:label="@string/title_activity_customer_profile"></activity>
        <activity
            android:name=".StarGasSplashViewActivity"
            android:label="@string/title_activity_star_gas_splash_view">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.CALL_PHONE">
    </uses-permission>
    <uses-permission android:name="android.permission.SEND_SMS">
    </uses-permission>
</manifest>
